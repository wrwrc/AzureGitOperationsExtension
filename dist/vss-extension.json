{
  "manifestVersion": 1,
  "id": "wrwrc-azure-git-operations-extension",
  "name": "Wrwrc Azure Git Operations",
  "version": "0.2.5",
  "publisher": "wrwrc",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Pipeline tasks of some Azure Git operations.",
  "categories": [
    "Azure Pipelines"
  ],
  "files": [
    {
      "path": "createAzureGitPRTask"
    },
    {
      "path": "createAzureGitTagTask"
    },
    {
      "path": "deleteAzureGitTagTask"
    }
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/wrwrc/AzureGitOperationsExtension"
  },
  "contributions": [
    {
      "id": "CreateAzureGitPRTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "CreateAzureGitPRTask"
      }
    },
    {
      "id": "CreateAzureGitTagTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "CreateAzureGitTagTask"
      }
    },
    {
      "id": "DeleteAzureGitTagTask",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "DeleteAzureGitTagTask"
      }
    }
  ]
}
